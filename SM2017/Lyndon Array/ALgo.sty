\def\@temp{nopython}\ifx\@tmp\@temp\global\@pythonfalse\relax\else%
\def\@temp{xml}\ifx\@tmp\@temp\global\@xmltrue\relax\else%
\@traiter\@tmp%
\fi\fi\fi\fi\fi\fi\fi\fi%
